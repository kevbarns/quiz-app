<div id="mySidenavLog" class="sidenav">
  <a href="javascript:void(0)" class="closebtn" onclick="closePanel()">&times;</a>
  <div class="container">
    <form action="/process-login" method="POST">
      <div class="form-row">
        <div class="form-group col-md-6">
          <label>
            Email:
            <input type="email" class="form-control" name="email" placeholder="enter email here" required />
          </label>
        </div>
        <div class="form-group col-md-6">
          <label>
            Password:
            <input type="password" class="form-control" name="originalPassword" placeholder="Enter password here"
              required />
          </label>
        </div>
      </div>
      <button class="btn btn-primary">Log In</button>
    </form>
  </div>
</div>